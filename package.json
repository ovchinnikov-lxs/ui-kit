{
    "name": "@ovchinnikov-lxs-frontend/ui-kit",
    "version": "2.4.4",
    "scripts": {
        "dev": "vite --port 3001",
        "build": "run-p type:check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "vite build",
        "type:check": "vue-tsc --noEmit",
        "el:check": "eslint --ext .ts --ext .vue .",
        "sl:check": "stylelint \"**/*.{css,scss,vue}\"",
        "lint:check": "npm run el:check && npm run sl:check",
        "el:fix": "eslint --ext .vue --ext .ts . --fix",
        "sl:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
        "lint:fix": "npm run el:fix && npm run sl:fix",
        "unit:test": "jest",
        "prepare": "husky install"
    },
    "dependencies": {
        "animejs": "^3.2.1",
        "lodash": "^4.17.21",
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/animejs": "^3.1.7",
        "@types/jest": "^29.4.0",
        "@types/lodash": "^4.14.191",
        "@types/node": "^16.18.11",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "husky": "^8.0.3",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.3.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.54.7",
        "stylelint": "^13.13.1",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.14.2",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vite-plugin-checker": "^0.5.6",
        "vite-plugin-dts": "^2.0.0-beta.3",
        "vite-plugin-stylelint": "^4.2.0",
        "vue-router": "^4.1.5",
        "vue-tsc": "^0.39.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ovchinnikov-lxs/ui-kit.git"
    },
    "publishConfig": {
        "@ovchinnikov-lxs-frontend:registry": "https://registry.npmjs.org"
    },
    "license": "ISC",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/ui-kit.umd.js",
    "module": "./dist/ui-kit.es.js",
    "exports": {
        ".": {
            "import": "./dist/ui-kit.es.js",
            "require": "./dist/ui-kit.umd.js"
        },
        "./dist/ui-kit.css": {
            "import": "./dist/ui-kit.css",
            "require": "./dist/ui-kit.css"
        }
    },
    "types": "./dist/main.d.ts"
}
